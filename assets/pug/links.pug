if helper.validArray(resume.basics.profiles)
  div(class='links margin-container-4 no-break')
    p(class='section-title no-alone-1') Links
    div(class='container')
      ul
        each profile in resume.basics.profiles
          li(class='article margin-text-4')
            a(href=profile.url)!=helper.beautifyLink(profile.url)
